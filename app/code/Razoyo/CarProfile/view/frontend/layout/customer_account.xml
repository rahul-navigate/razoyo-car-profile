<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="customer_account_navigation">
            <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-navigation-navigate-link">
                <arguments>
                    <argument name="label" xsi:type="string">My Car Profile</argument>
                    <argument name="path" xsi:type="string">carprofile/customer/mycar</argument>
                     <argument name="sortOrder" xsi:type="number">190</argument>
                </arguments>
            </block>
        </referenceBlock>
        <move element="customer-account-navigation-navigate-link" destination="customer_account_navigation" before="-"/>
    </body>
</page>
